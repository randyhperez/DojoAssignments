<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Errors</title>
  </head>
  <body>
    <h3>Error</h3>
    <% if(typeof(errors) !== 'undefined') { %>
      <% for (const error of errors) { %>
        <p><%= error  %></p>
      <% } %>
    <% } %>
    <a href="/">Home</a>
  </body>
</html>
